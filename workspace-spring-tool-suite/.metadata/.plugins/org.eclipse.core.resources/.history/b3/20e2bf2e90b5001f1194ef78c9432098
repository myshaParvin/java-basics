package javadatatypes;

public class primitivedatatypes {

	public static void main(String[] args) {
		int number = 16597;
		float number2 = 15.6f;
		double number3 = 87483748;
		char letter = 'M';
		boolean bool= true;
		
		System.out.println("Int number expressed as "+number+"\nFloat expressed as "+number2+ 
				"Double expressed as "+number3+ "\nCharacter expressed as "+letter+ "\nBoolean expressed as "+bool);
	}

}
